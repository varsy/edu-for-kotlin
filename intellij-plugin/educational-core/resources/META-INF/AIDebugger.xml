<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="Educational.BreakpointTypeManager"
                    beanClass="com.intellij.lang.LanguageExtensionPoint"
                    dynamic="true">
      <with attribute="implementationClass"
            implements="com.jetbrains.edu.learning.aiDebugging.breakpoint.BreakpointTypeManager"/>
    </extensionPoint>
  </extensionPoints>
  <extensions defaultExtensionNs="com.intellij">
    <postStartupActivity implementation="com.jetbrains.edu.learning.aiDebugging.breakpoint.AIBreakPointsStartupActivity"/>
  </extensions>
  <actions>
    <action id="Educational.AIDebug"
            class="com.jetbrains.edu.learning.aiDebugging.actions.DebugAction">
      <add-to-group group-id="EditorPopupMenu" anchor="last"/>
    </action>
  </actions>
  <resource-bundle>messages.EduCoreBundle</resource-bundle>
</idea-plugin>
