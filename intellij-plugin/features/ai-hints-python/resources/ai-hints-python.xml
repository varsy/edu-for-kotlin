<idea-plugin package="com.jetbrains.edu.aiHints.python">
  <dependencies>
    <!--suppress PluginXmlValidity -->
    <module name="ai-hints-core"/>
    <plugin id="com.intellij.modules.python"/>
  </dependencies>

  <extensions defaultExtensionNs="aiHints">
    <functionSignaturesProvider language="Python" implementationClass="com.jetbrains.edu.aiHints.python.PyFunctionSignaturesProvider"/>
    <functionSignatureResolver language="Python" implementationClass="com.jetbrains.edu.aiHints.python.PyFunctionSignatureResolver"/>
    <filesDiffer language="Python" implementationClass="com.jetbrains.edu.aiHints.python.PyFilesDiffer"/>
    <inspectionProvider language="Python" implementationClass="com.jetbrains.edu.aiHints.python.PyInspectionProvider"/>
  </extensions>
</idea-plugin>